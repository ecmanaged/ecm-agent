#! /bin/sh
# prerm script for ack-backup
#
# see: dh_installdeb(1)

set -e

# Stop ecagent 
INIT=/opt/ecmanaged/ecagent/init
if [ -x ${INIT} ]; then
  ${INIT} stop
fi

case "$1" in
  remove)
  ;;
  upgrade|deconfigure)
  ;;
  failed-upgrade)
  ;;
  *)
    echo "prerm called with unknown argument \`$1'" >&2
    exit 1
  ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0


